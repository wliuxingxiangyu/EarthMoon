<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bn.Sample7_4"
    android:versionCode="1"
    android:versionName="8.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="21" >
    </uses-sdk>

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.bn.Sample7_4.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bn.Sample7_4.MySurfaceActivity" >
        </activity>
        <activity android:name="com.hz.downloadclient.AIDLDownloadClientActivity" >
        </activity>

        <service android:name="com.hz.aidl.DownloadService" >
            <intent-filter>
                <action android:name="com.hz.process.aidl.DownloadService" />
            </intent-filter>
        </service>
    </application>

</manifest>